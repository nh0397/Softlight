# LLM Provider: "gemini" or "huggingface"
LLM_PROVIDER=gemini

# Google Gemini API Key (required if LLM_PROVIDER=gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini Model Name
GEMINI_MODEL=gemini-2.5-flash

# Hugging Face API Key (optional, for higher rate limits)
# Get one at https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Hugging Face Model (used if LLM_PROVIDER=huggingface)
HUGGINGFACE_MODEL=Qwen/Qwen3-VL-8B-Instruct

# Browser settings
HEADLESS=false
BROWSER_TYPE=firefox

